  "description": "A custom OSL bot which helps with OSL weekly status reports of members.",
  "main": "app.ts",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon dist/app",
    "watch": "tsc -w",
    "build": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "firebase-admin": "^10.0.2",
